package com.csci5980.primeevils;

public class ListItem {
	private int id;
	private int quantity;
	
	private String item_name;
	private String bought_by;
	private String shop_name;
	private String user_name;
	
	ListItem() {
		id = 0; quantity = 0;
		item_name = "";
		bought_by = "";
		shop_name = "";
		user_name = "";
	}
	
	public int getId() { return id; }
	public int getQuantity() { return quantity; }
	public String getItemName() { return item_name; }
	public String getBoughtBy() { return bought_by;	}
	public String getShopName() { return shop_name; }
	public String getUserName() { return user_name; }
	
	public void setId(int id) { this.id = id; }
	public void setQuantity(int qty) { this.quantity = qty; }
	public void setItemName(String item) { this.item_name = item; }
	public void setBought(String bought) { this.bought_by = bought; }
	public void setShopName(String shop) { this.shop_name = shop; }
	public void setUserName(String name) { this.user_name = name; }
}
